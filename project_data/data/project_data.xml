<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--Running state-->
        <record id="project_stage_running" model="project.task.type">
            <field name="sequence">1</field>
            <field name="name">Running</field>
        </record>
        <!--Done state-->
        <record id="project_stage_done" model="project.task.type">
            <field name="sequence">2</field>
            <field name="name">Done</field>
        </record>
        <!--Electrical project-->
        <record id="project_project_electrical" model="project.project">
            <field name="name">Electrical</field>
            <field name="color">3</field>
            <field name="user_id" ref="base.user_admin"/>
            <field name="type_ids"
                   eval="[(4, ref('project_data.project_stage_running')),(4, ref('project_data.project_stage_done'))]"/>
            <field name="privacy_visibility">portal</field>
        </record>
        <!--Workshop Project-->
        <record id="project_project_workshop" model="project.project">
            <field name="name">Workshop</field>
            <field name="color">3</field>
            <field name="user_id" ref="base.user_admin"/>
            <field name="type_ids"
                   eval="[(4, ref('project_data.project_stage_running')),(4, ref('project_data.project_stage_done'))]"/>
            <field name="privacy_visibility">portal</field>
        </record>
        <!--Mechanical Project-->
        <record id="project_project_mechanical" model="project.project">
            <field name="name">Mechanical</field>
            <field name="color">4</field>
            <field name="user_id" ref="base.user_admin"/>
            <field name="type_ids"
                   eval="[(4, ref('project_data.project_stage_running')),(4, ref('project_data.project_stage_done'))]"/>
            <field name="privacy_visibility">portal</field>
        </record>
        <!--Generator Project-->
        <record id="project_project_generator" model="project.project">
            <field name="name">Generator</field>
            <field name="color">3</field>
            <field name="user_id" ref="base.user_admin"/>
            <field name="type_ids"
                   eval="[(4, ref('project_data.project_stage_running')),(4, ref('project_data.project_stage_done'))]"/>
            <field name="privacy_visibility">portal</field>
        </record>
    </data>
</odoo>