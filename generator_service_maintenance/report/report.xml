<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <report
            id="generator_document"
            string="Service Report"
            model="generator.service"
            report_type="qweb-pdf"
            name="generator_service_maintenance.report_generator_document"
            file="generator_service_maintenance.report_generator_document"
            print_report_name="'Service Report - %s' % (object.name)"
    />

    <report
            id="generator_service_monthly"
            string="Monthly Report"
            model="generator.service"
            report_type="qweb-pdf"
            name="generator_service_maintenance.monthly_service_report_doc"
            file="generator_service_maintenance.monthly_service_report_doc"
            menu="False"
    />

    <report id="action_report_material_request"
            string="Print"
            model="generator.material.request"
            report_type="qweb-pdf"
            name="generator_service_maintenance.material_request_report"
            file="generator_service_maintenance.material_request_report"
            paperformat="cy_timesheet_payroll.paperformat_report"
            menu="False"
    />

    <report id="action_report_service_status"
            string="Service Status Report"
            model="generator.service"
            report_type="qweb-pdf"
            name="generator_service_maintenance.status_template"
            file="generator_service_maintenance.status_template"
            paperformat="cy_timesheet_payroll.paperformat_report"
            menu="False"
    />

    <report id="action_report_service_completion"
            string="Service Completion"
            model="service.completion"
            report_type="qweb-pdf"
            name="generator_service_maintenance.report_service_completion"
            file="generator_service_maintenance.report_service_completion"
            paperformat="cy_timesheet_payroll.paperformat_report"
            print_report_name="'Service Completion - %s' % (object.name).replace('/', '')"
    />


</odoo>
