<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <template id="monthly_service_report_doc">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <div class="page">
                        <center>
                            <h1>Monthly Service Report</h1>
                        </center>
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>FSD Number</th>
                                    <th>Service Perform Dates</th>
                                    <th>Monthly</th>
                                </tr>
                            </thead>
                            <tbody>
                                <t t-foreach="docs" t-as="line">
                                    <tr>
                                        <td>
                                            <span t-esc="line['fsd_number']"/>
                                        </td>
                                        <td>
                                            <span t-esc="line['service_dates']"/>
                                        </td>
                                        <td>
                                            <span t-esc="line['count']"/>&amp;nbsp;&amp;nbsp;Times
                                        </td>
                                    </tr>
                                </t>
                            </tbody>
                        </table>
                    </div>
                </t>
            </t>
        </template>
    </data>
</odoo>