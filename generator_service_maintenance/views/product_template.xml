<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="product_template_generator_location">
        <field name="name">product.template.form.generator.location</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='stock_property']" position="inside">
                <field name="generator_stock_location"/>
            </xpath>
        </field>
    </record>
</odoo>