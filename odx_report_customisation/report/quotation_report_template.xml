                                                <t t-set="heading_number" t-value="heading_number+1"/>

                                                <tr align="center">
                                                    <td width="5%" name="th_no" class="text-center ">Sl.No
                                                    </td>
                                                    <td width="15%" name="th_description"
                                                        class="text-center ">Type
                                                        of Service
                                                    </td>
                                                    <td name="th_qty" class="text-center ">Qty
                                                    </td>
                                                    <td name="th_unitprice" class="text-center ">Unit
                                                        Price
                                                    </td>
                                                    <t t-set="dis_count" t-value="0"/>
                                                    <t t-foreach="o.work_quotation_line_ids" t-as="line">
                                                        <t t-if="not line.display_type == 'line_section'">
                                                            <t t-if="line.discount>0">
                                                                <t t-set="dis_count" t-value="dis_count + 1"/>
                                                            </t>
                                                        </t>
                                                    </t>
                                                    <t t-if="dis_count>0">
                                                        <td name="th_discount" class="text-center ">Disc</td>
                                                    </t>
                                                    <td name="th_netprice" class="text-center ">Net
                                                        Price
                                                    </td>
                                                    <td name="th_vat" class="text-center ">VAT Amount</td>
                                                    <td name="th_total" class="text-center ">Total
                                                    </td>
                                                </tr>
                                            </t>
                                        </t>


                                        <tr align="center">
                                            <t t-set="sl" t-value="sl + 1"/>
