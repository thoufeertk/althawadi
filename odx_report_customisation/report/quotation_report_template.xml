                                                        <td name="th_discount" class="text-center ">Disc</td>
                                                    </t>
                                                    <td name="th_netprice" class="text-center ">Net
                                                        Price
                                                    </td>
                                                    <td name="th_vat" class="text-center ">VAT Amount</td>
                                                    <td name="th_total" class="text-center ">Total
                                                    </td>
                                                </tr>
                                            </t>
                                        </t>


                                        <tr align="center">
                                            <t t-set="sl" t-value="sl + 1"/>
                                            <td>
                                                <span t-esc="sl"/>
                                            </td>
                                            <td>
                                                <span t-field="line.product_id"/>
                                            </td>
                                            <td>
                                                <span t-field="line.product_uom_qty"/>
                                            </td>
                                            <td>
                                                <span t-esc="'%.3f'% line.price_unit"/>
                                            </td>
                                            <t t-if="dis_count>0">
                                                <td>
                                                    <span t-esc="'%.3f'% line.discount"/>

                                                    <t t-set="total_discount"
                                                       t-value="total_discount + ((line.price_unit * line.discount)/100)"/>
                                                </td>
                                            </t>

                                            <td>
                                                <span t-esc="'%.3f'% line.price_subtotal"/>
                                            </td>
                                            <td>
                                                <span t-esc="'%.3f'% line.price_tax"/>
                                            </td>
                                            <td>
                                                <span t-esc="'%.3f'% line.price_total"/>
                                            </td>
                                        </tr>
                                    </t>
                                </t>
                            </table>
                            <br/>


                            <table style="height: 60px; width: 380px; margin-left:550px;" class="table table_border">
                                <tr>
                                    <td style="text-align:center">
                                        Amount Untaxed :
                                    </td>
                                    <td style="text-align:right">
                                        <t t-esc="o.amount_untaxed"
                                           t-options='{"widget": "monetary", "display_currency": o.currency_id}'/>
                                    </td>
                                </tr>
                                <tr>
                                    <t t-foreach="o.tax_group()" t-as="tax">
                                        <tr>
                                            <td style="text-align:center">
                                                <span>Value Added</span>
                                                <t t-esc="tax['name']"/>:
                                            </td>
                                            <td style="text-align:right">
                                                <t t-esc="tax['tax_amount']"
                                                   t-options='{"widget": "monetary", "display_currency": o.currency_id}'/>
                                            </td>
                                        </tr>
                                    </t>
                                </tr>

                                <t t-if="total_discount>0">
                                    <tr>
                                        <td style="text-align:center">
                                            Total Discount :
                                        </td>
                                        <td style="text-align:right">
                                            <t t-esc="'%.3f'% total_discount"/>
                                            <t t-esc="o.currency_id.symbol"/>
                                        </td>
                                    </tr>
                                </t>


                                <tr>
                                    <th style="text-align:center">
                                        Amount Total :
                                    </th>
                                    <td style="text-align:right">
                                        <t t-esc="o.amount_total"
                                           t-options='{"widget": "monetary", "display_currency": o.currency_id}'/>
                                    </td>
                                </tr>
                            </table>
                            <table width="100%">
                                <tr width="100%">
                                    <b>Scope of Work</b>
                                </tr>
                                <br/>
                                <tr width="100%" style="font-size: 15px;padding-left: 15px">
                                    <span t-field="o.scope_of_work_description"/>
                                </tr>
                                <br/>
                                <br/>

                                <tr width="100%">
                                    <b>Testing &amp; Inspection</b>
                                </tr>
                                <br/>
                                <tr width="100%" style="font-size: 15px;padding-left: 15px">
                                    <span t-field="o.odx_testing.description"/>
                                </tr>
                                <br/>
                                <br/>

                                <tr width="100%">
                                    <b>Delivery</b>
                                </tr>
                                <br/>
                                <tr width="100%" style="font-size: 15px;padding-left: 15px">
                                    <span t-field="o.odx_terms_and_condition.description"/>
                                </tr>
                                <br/>
                                <br/>

                                <tr width="100%">
                                    <b>Payment Terms</b>
                                </tr>
                                <br/>
                                <tr width="100%" style="font-size: 15px;padding-left: 15px">
                                    <span t-field="o.odx_payment_term.description"/>
                                </tr>
                                <br/>
                                <br/>

                                <tr width="100%">
                                    <b>Extend of Quote</b>
                                </tr>
                                <br/>
                                <tr width="100%" style="font-size: 15px;padding-left: 15px">
                                    <span t-field="o.odx_extend_of_quote.description"/>
                                </tr>
                                <br/>
                                <br/>

                                <tr width="100%">
                                    <b>Warranty</b>
                                </tr>
                                <br/>
                                <tr width="100%" style="font-size: 15px;padding-left: 15px">
                                    <span t-field="o.odx_warranty.description"/>
                                </tr>
                                <br/>


                            </table>

                            <br/>
                            <table>
                                <br/>
                                <br/>
                            </table>
                            <br/>
                            <table width="100%">
                                <tr>
                                    <td style="float:left;width:25%;text-align:center;border-top: 2px solid black">
                                        <b>Prepared By:</b>
                                    </td>
                                    <td style="float:right;width:25%;text-align:center;border-top: 2px solid black">
                                        <b>Approved By</b>
                                    </td>
                                    <br/>
                                    <br/>
                                    <tr>
                                        <td>Name :
                                            <t t-esc="o.signed_name"/>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>Telephone :
                                            <t t-esc="o.telephone"/>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td>
                                            Signature :
                                            <t t-if="o.digital_signature">
                                                <img t-att-src="image_data_uri(o.digital_signature)"
                                                     style="max-height: 4cm; max-width: 8cm;"/>
                                            </t>
                                        </td>
                                    </tr>

                                </tr>
                            </table>
                        </div>
                    </t>
                </t>
            </t>
        </t>
    </template>
</odoo>
