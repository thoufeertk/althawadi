<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="workshop_project_stage_running" model="project.task.type">
            <field name="sequence">1</field>
            <field name="name">Running</field>
        </record>
        <record id="workshop_project_stage_done" model="project.task.type">
            <field name="sequence">2</field>
            <field name="name">Done</field>
        </record>
        <record id="project_project_workshop" model="project.project">
            <field name="name">Workshop</field>
            <field name="color">3</field>
            <field name="user_id" ref="base.user_admin"/>
            <field name="type_ids"
                   eval="[(4, ref('workshop.workshop_project_stage_running')),(4, ref('workshop.workshop_project_stage_done'))]"/>
            <field name="privacy_visibility">portal</field>
        </record>
    </data>
</odoo>