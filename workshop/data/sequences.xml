<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for workshop -->
        <record id="enquiry_sequence" model="ir.sequence">
            <field name="name">Enquiry Register</field>
            <field name="code">enquiry.register.code</field>
            <field name="prefix">ENQ</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="initial_sequence" model="ir.sequence">
            <field name="name">Initial Inspection</field>
            <field name="code">initial.inspection.code</field>
            <field name="prefix">INS</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="contract_sequence" model="ir.sequence">
            <field name="name">Contract</field>
            <field name="code">workshop.contract.code</field>
            <field name="prefix">AWC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="mechanical_sequence" model="ir.sequence">
            <field name="name">Mechanical</field>
            <field name="code">mechanical.report.code</field>
            <field name="prefix">MR</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="workshop_quotation_sequence" model="ir.sequence">
            <field name="name">Quotation</field>
            <field name="code">workshop.quotation.code</field>
            <field name="prefix">WQ</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="workshop_order_sequence" model="ir.sequence">
            <field name="name">Work Order</field>
            <field name="code">workshop.order.code</field>
            <field name="prefix">WO</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="delivery_note_sequence" model="ir.sequence">
            <field name="name">Delivery Note</field>
            <field name="code">delivery.note.code</field>
            <field name="prefix">DN</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--        Sequence for Material Request-->
        <record id="workshop_material_request_sequence" model="ir.sequence">
            <field name="name">Material Request</field>
            <field name="code">workshop.material.request</field>
            <field name="prefix">WMR</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>
